generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paycheck {
  id        String   @id @default(cuid())
  amount    Int
  payType   Pay
  date      DateTime 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Pay {
  TIP
  SALARY
  BONUS
  NONE
}
